<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="rootPane" prefHeight="307.0" prefWidth="430.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.YouthController">
    <children>
        <Text layoutX="20.0" layoutY="27.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Какое у вас образование:" textOrigin="CENTER" wrappingWidth="388.984375">
            <font>
                <Font size="20.0" />
            </font>
        </Text>
      <ComboBox fx:id="comboBox" layoutX="21.0" layoutY="55.0" onAction="#getLine" prefWidth="150.0" promptText="Ваше образование" />
        <ImageView fitHeight="195.0" fitWidth="227.0" layoutX="194.0" layoutY="47.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/education.png" />
         </image></ImageView>
        <Button fx:id="btnGoBackScreen" layoutX="21.0" layoutY="258.0" mnemonicParsing="false" onAction="#goBack" prefWidth="150.0" text="Назад" />
        <Button fx:id="btnNextScreen" layoutX="260.0" layoutY="258.0" mnemonicParsing="false" onAction="#loadNextScreen" prefWidth="150.0" text="Вперед" />
      <Label fx:id="labelSelected" layoutX="21.0" layoutY="226.0" text="Выбрано:" />
    </children>
</AnchorPane>
